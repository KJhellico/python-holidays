#. %s (estimated).
#, c-format
msgid "%s (болжолдуу)"
msgstr "%s (приблизительная дата)"

#. %s (observed).
#, c-format
msgid "%s (көрүлгөн күнү)"
msgstr "%s (выходной)"

#. %s (observed, estimated).
#, c-format
msgid "%s (көрүлгөн күнү, болжолдуу)"
msgstr "%s (выходной, приблизительная дата)"

#. New Year's Day.
msgid "Жаңы жыл"
msgstr "Новый год"

#. New Year Holidays.
msgid "Жаңы жылдык каникулдар"
msgstr "Новогодние каникулы"

#. Christmas Day.
msgid "Ыйса Пайгамбардын туулган күнү"
msgstr "Рождество Христово"

#. Fatherland Defender's Day.
msgid "Ата-Журтту коргоонун күнү"
msgstr "День защитника Отечества"

#. International Women's Day.
msgid "Аялдардын эл аралык күнү"
msgstr "Международный женский день"

#. Women's Day.
msgid "Аялдар күнү"
msgstr "День женщин"

#. Nooruz Holiday.
msgid "Элдик Нооруз майрамы"
msgstr "Народный праздник Нооруз"

#. Day of the People's Revolution.
msgid "Элдик революция күнү"
msgstr "День народной революции"

#. Day of the People's April Revolution.
msgid "Элдик Апрель революциясы күнү"
msgstr "День народной Апрельской революции"

#. Labor Day.
msgid "Эмгек майрамы"
msgstr "Праздник труда"

#. International Workers' Solidarity Day.
msgid "Эмгекчилердин эл аралык тилектештик күнү"
msgstr "День международной солидарности трудящихся"

#. Constitution Day.
msgid "Кыргыз Республикасынын Конституция күнү"
msgstr "День Конституции Кыргызской Республики"

#. May Holidays.
msgid "Май каникулдары"
msgstr "Майские каникулы"

#. Victory Day.
msgid "Жеңиш күнү"
msgstr "День Победы"

#. Independence Day.
msgid "Кыргыз Республикасынын Көз карандысыздыгынын күнү"
msgstr "День независимости Кыргызской Республики"

#. Days of History and Commemoration of Ancestors.
msgid "Тарых жана ата-бабаларды эскерүү күндөрү"
msgstr "Дни истории и памяти предков"

#. Day of the Great October Socialist Revolution.
msgid "Улуу Октябрь социалисттик революциясынын күнү"
msgstr "День Великой Октябрьской социалистической революции"

#. Eid al-Fitr.
msgid "Орозо айт"
msgstr "Орозо айт"

#. Eid al-Adha.
msgid "Курман айт"
msgstr "Курман айт"

#. Fatherland Defender's Day.
msgid "Мекенди коргоочулардын күнү"
msgstr "День защитника Отечества"

#. Substituted date format.
msgid "%d.%m.%Y"
msgstr "%d.%m.%Y"

#. Day off (substituted from %s).
#, c-format
msgid "Эс алуу күнү (%s күнүнөн которулган)"
msgstr "Выходной (перенесено с %s)"

#. Additional public holiday.
msgid "Кошумча эс алуу күнү"
msgstr "Дополнительный выходной"
